{
  "version": "0.2.0",
  "configurations": [
    // ========================
    // VENOM PROJECT - PYTHON
    // ========================
    {
      "name": "üêç Venom: Main (Interactive)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/project_Venom/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/project_Venom"
      },
      "python": "${workspaceFolder}/project_Venom/.venv/Scripts/python.exe"
    },
    {
      "name": "üî• Venom: Daemon (Background)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/project_Venom/venom_daemon.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/project_Venom"
      }
    },
    {
      "name": "üëÅÔ∏è Venom: HUD (Streamlit)",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/project_Venom/venom_hud.py"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom"
    },
    {
      "name": "üåê Venom: Web Server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/project_Venom/web_server.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom"
    },
    {
      "name": "üß™ Venom: Phase 2 Test",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/project_Venom/test_phase2.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom"
    },
    {
      "name": "üß† Debug: Brain Module Only",
      "type": "debugpy",
      "request": "launch",
      "module": "brain.system_brain",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/project_Venom"
      }
    },
    {
      "name": "‚ö° Debug: Current Python File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${fileDirname}"
    },
    {
      "name": "üî¨ Venom: Pytest (All Tests)",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "-v",
        "-s",
        "${workspaceFolder}/project_Venom/tests"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/project_Venom"
    },
    {
      "name": "üî¨ Venom: Pytest (Current File)",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "-v",
        "-s",
        "${file}"
      ],
      "console": "integratedTerminal",
      "justMyCode": false
    },

    // ========================
    // JARVIS PROJECTS
    // ========================
    {
      "name": "ü§ñ Jarvis Advanced",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/[GET INSPIRATION]/jarvis-advanced/backend/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/[GET INSPIRATION]/jarvis-advanced"
    },
    {
      "name": "ü§ñ Jarvis 2025",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/[GET INSPIRATION]/Jarvis-2025-master/main.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/[GET INSPIRATION]/Jarvis-2025-master"
    },

    // ========================
    // WEB/NODE CONFIGURATIONS
    // ========================
    {
      "name": "üåê Launch Chrome (Angular)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/project_Venom/frontend",
      "sourceMaps": true
    },
    {
      "name": "üåê Launch Chrome (Venom Interface)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8000/venom_interface.html",
      "webRoot": "${workspaceFolder}/project_Venom"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/app.js"
    },

    // ========================
    // ATTACH CONFIGURATIONS
    // ========================
    {
      "name": "üîó Python: Attach to Process",
      "type": "debugpy",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": false
    },
    {
      "name": "üîó Python: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/project_Venom",
          "remoteRoot": "."
        }
      ]
    }
  ],
  "compounds": [
    {
      "name": "üöÄ Venom: Full System",
      "configurations": [
        "üêç Venom: Main (Interactive)",
        "üî• Venom: Daemon (Background)",
        "üëÅÔ∏è Venom: HUD (Streamlit)"
      ],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "Venom",
        "order": 1
      }
    }
  ]
}
